{
  "name": "A browser action with a popup that excute auto review",
  "description": "auto review",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "default_title": "auto review",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "js": ["contentscript.js"],
    "matches": ["<all_urls>"]
  }],
  "manifest_version": 2,
  "web_accessible_resources": ["auto_review/review_functions.js"]
}
